\newcommand{\crefpreandpostname}[5]{%
  \crefformat{#1}{#2##2##1##3#3}%
  \crefrangeformat{#1}{#4##3##1##4\crefrangeconjunction##5##2##6#5}%
  \crefmultiformat{#1}{#4##2##1##3}{\crefpairconjunction##2##1##3#5}{\crefmiddleconjunction##2##1##3}{\creflastconjunction##2#1#3#5}%
  \crefmultiformat{#1}{#4##2##1##3}{\crefpairconjunction##2##1##3#5}{\crefmiddleconjunction##2##1##3}{\creflastconjunction##2#1#3#5}%
}
\newcommand{\Crefpreandpostname}[5]{%
  \Crefformat{#1}{#2##2##1##3#3}%
  \Crefrangeformat{#1}{#4##3##1##4\crefrangeconjunction##5##2##6#5}%
  \Crefmultiformat{#1}{#4##2##1##3}{\crefpairconjunction##2##1##3#5}{\crefmiddleconjunction##2##1##3}{\creflastconjunction##2#1#3#5}%
  \Crefmultiformat{#1}{#4##2##1##3}{\crefpairconjunction##2##1##3#5}{\crefmiddleconjunction##2##1##3}{\creflastconjunction##2#1#3#5}%
}

